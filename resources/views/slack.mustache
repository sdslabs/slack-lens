<html>
<head>
  <meta charset="utf-8"/>
  <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
  <title></title>
  <!--<link href="https://fonts.googleapis.com/css?family=Lato:400,700,900" rel="stylesheet" type="text/css"/>-->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <link href="slack.css" rel="stylesheet" type="text/css"/>

</head>
<body style="overflow:hidden;">
  <div   id="mySidebar" style="overflow:scroll;">
    <button onclick="w3_close()">Close &times;</button>
    <div id="sidebar" style="padding-left:20px;"></div>
  </div>

  {{#data}}
  <div class="header">
    <div class="team-menu">{{slack-name}}</div>
    <div id="header_">

      <h2 id="channel-title">
        #{{active}}
      </h2>

      <div id="rightIcons">
        <div id="wrapper1">
          <i class="box_ phone fa fa-phone fa-lg" aria-hidden="true"></i>
          <i class="box_ cog fa fa-cog fa-lg" aria-hidden="true"></i>
          <i class="box_  details fa fa-list-alt fa-lg" aria-hidden="true"></i>
        </div>
        <div id="wrapper2">
          <i class="box_ search-glass fa fa-search" aria-hidden="true"></i>
          <input id="search" placeholder="dd/mm/yyyy" type="text">
          <input id="number" type="number" value="0">
        </div>
        <div id="wrapper3">
          <i class="box_ at fa fa-at fa-lg" aria-hidden="true"></i>
          <i class="box_ star-o fa fa-star-o fa-lg" aria-hidden="true"></i>
          <i class="box_ menu fa fa-ellipsis-v fa-lg" aria-hidden="true"></i>
        </div>
      </div>

      <div id="icons">
        <i class="box_ star fa fa-star-o" aria-hidden="true"></i>
        <h3 id="line"> | </h3>
        <i class="box_ user fa fa-user-o" aria-hidden="true"></i>
        <h3 id="line2"> | </h3>
        <i class="box_ pin fa fa-thumb-tack" aria-hidden="true"></i>
      </div>

    </div>
  </div>
  <div class="main">
    <div class="listings">
      <div class="listings_channels">
        <h2 class="listings_header">Channels</h2>
        <ul class="channel_list">
          {{#channels}}
          {{#_source}}
          <li class="channel" id="{{name}}" onclick="location='slack-lens?channel={{name}}'"><a class="channel_name"><span><span class="prefix"># </span> {{name}}</span></a></li>
          {{/_source}}
          {{/channels}}
        </ul>
      </div>
    </div>
    <div class="message-history">
    </div>
    <div class="footer">
      <div class="user-menu"><span class="user-menu_profile-pic"></span><span class="user-menu_username">Bot</span><img class="connection_icon" src=""/><span class="connection_status">online</span></div>
      <div class="input-box">
        <input class="input-box_text" type="text"/>
      </div>
    </div>
    <script>
      var active="{{active}}";
    </script>
    <script src="/v1/slack.js">
    </script>
    {{/data}}
  </body>
  </html>